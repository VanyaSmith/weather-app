{
  "presets": [
    [
      "next/babel",
      {
        "preset-react": {
          "runtime": "automatic",
          "importSource": "@emotion/react"
        }
      }
    ]
  ],
  "plugins": [
    "@emotion/babel-plugin",
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "./src/components",
          "modules": "./src/modules",
          "interfaces": "./src/interfaces",
          "utils": "./src/utils",
          "src": "./src"
        }
      }
    ]
  ]
}
